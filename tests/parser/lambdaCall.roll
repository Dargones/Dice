RUN: %parser "%s" > "%t"
RUN: %diff "%s.expect" "%t"
*/
// Declarations
[]->Int zero;
[Int, Int]->Int sum;
[Int] -> [[Int] -> Int] curriedSum;

zero = lambda () -> Int {
    return 0;
};

sum = lambda (Int x, Int y)->Int {
    return x + y;
};

curriedSum = lambda (Int x)->[[Int] -> Int] {
    return lambda(Int y)->Int {
        return x + y;
    };
};

zero();
sum(1, 2);
curriedSum(1)(2);